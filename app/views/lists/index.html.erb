
<h1>Hello, this is your watch list</h1>


<ul>
  <% @lists.each do |list| %>
    <div class="card" style="width: 18rem;">
      <div class="card-body">
        <h5 class="card-title"><%= list.name %></h5>
        <a href="#" class="card-link"><%=link_to 'Details', list_path(list)%></a>
      </div>
    </div>
  <% end %>
</ul>

<%=link_to 'Add a new list', new_list_path, class:"btn btn-primary" %>
